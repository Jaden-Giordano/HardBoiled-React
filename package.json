{
  "name": "hardboiled-react",
  "version": "1.0.0",
  "description": "A simple ReactJS/Parket boilerplate containing Webpack, Sass, ES6, XO w/ Airbnb style guide, and Ava (w/ Enzyme)!",
  "main": "src/index.jsx",
  "author": {
    "name": "Jaden Giordano",
    "email": "jaden@forester.io"
  },
  "license": "MIT",
  "scripts": {
    "dev": "webpack-dev-server --mode development --watch",
    "build": "webpack --mode production",
    "xo": "xo",
    "ava": "ava",
    "test": "xo && ava"
  },
  "xo": {
    "envs": [
      "browser"
    ],
    "parser": "babel-eslint",
    "extends": "airbnb",
    "rules": {
      "no-param-reassign": "off"
    }
  },
  "ava": {
    "files": "tests/**/*.{js,jsx}",
    "require": [
      "@babel/polyfill",
      "@babel/register"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.42",
    "@babel/polyfill": "^7.0.0-beta.42",
    "@babel/preset-env": "^7.0.0-beta.42",
    "@babel/register": "^7.0.0-beta.42",
    "ava": "git://github.com/Jaden-Giordano/ava#comma-delimited-filters",
    "babel-eslint": "^8.2.2",
    "css-loader": "^0.28.11",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.19.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "jsdom": "^11.6.2",
    "node-sass": "^4.7.2",
    "sass-loader": "^6.0.7",
    "style-loader": "^0.20.3",
    "webpack": "^4.1.1",
    "webpack-cli": "^2.0.12",
    "webpack-dev-server": "^3.1.1",
    "xo": "^0.20.3"
  },
  "dependencies": {
    "@babel/preset-react": "^7.0.0-beta.42",
    "@babel/preset-stage-0": "^7.0.0-beta.42",
    "parket": "^0.4.0",
    "prop-types": "^15.6.1",
    "react": "^16.2.0",
    "react-dom": "^16.2.0"
  }
}
